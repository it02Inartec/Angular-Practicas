<!DOCTYPE html>
<html>
<script src="http://ajax.googleapis.com/ajax/libs/angularjs/1.4.8/angular.min.js"></script>
<body ng-app="myApp">

<div ng-app="" ng-init="names=['Esto','Es','Super','Facil','jojojo']"></div>
<super-facil></super-facil>

<div super-facil></div>

<script>

var app = angular.module("myApp", []);
app.directive("superFacil", function() {
    return {
        restrict : "A",
        template : '<ul><li ng-repeat="x in names" ng-bind="x"></li></ul>'
    };
});
</script>
</body>
</html>
